{
    "MODE": "train",
    "MODEL": {
        "TYPE": "pretrained",
        "TRANSFORMER_MODEL": "dmis-lab/biobert-base-cased-v1.2",
        "HEADS": {
            "category": {"htype": "single_label", "num_labels": 7, "schema": "BIO"},
            "description": {"htype": "multi_label", "num_labels": 22, "schema": "BIO"},
            "experiment_type": {"htype": "multi_label", "num_labels": 22, "schema": "BIO"},
            "species": {"htype": "multi_label", "num_labels": 18, "schema": "BIO"},
            "phenotype": {"htype": "multi_label", "num_labels": 58, "schema": "BIO"},
            "activity": {"htype": "multi_label", "num_labels": 14, "schema": "BIO"}
        },
        "TAG_LIST_DEFINITIONS": "tag_list_definitions.json",
        "MAX_TOKEN_LENGTH": 512,
        "VERBOSE": 1
    },
    "DATA": {
        "PATH": "json_data/all_marked_test.json",
        "FILETYPE": "json_fgeom",
        "BATCH_SIZE": 1,
        "BALANCE_CLASSES": 0.5,
        "VERBOSE": 2
    },
    "TRAINING": {
        "TRAINED_NAME": "DistilBERT_multi_output_test",
        "OPTIMIZER_CLASS": "AdamW",
        "OPTIMIZER_KWARGS": {
            "lr": 3e-5,
            "eps": 1e-8
        },
        "FULL_FINETUNING": true,
        "EPOCHS": 1,
        "MAX_GRAD_NORM": 1,
        "VERBOSE": 3
    }
}
